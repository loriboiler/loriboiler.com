{
  "id": "accordion",
  "type": "SharedSlice",
  "name": "Accordion",
  "description": "Accordion",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "imageUrl": "",
      "primary": {
        "title": {
          "type": "StructuredText",
          "config": {
            "label": "Title",
            "placeholder": "",
            "allowTargetBlank": true,
            "single": "heading2"
          }
        }
      },
      "items": {
        "trigger": {
          "type": "Text",
          "config": {
            "label": "Trigger",
            "placeholder": ""
          }
        },
        "content": {
          "type": "StructuredText",
          "config": {
            "label": "Content",
            "placeholder": "This text answers the trigger question",
            "allowTargetBlank": true,
            "multi": "paragraph,strong,em,hyperlink,image,embed,list-item,o-list-item"
          }
        },
        "link": {
          "type": "Link",
          "config": {
            "label": "Link",
            "placeholder": "Optionally include a button or link",
            "allowTargetBlank": false,
            "select": null
          }
        },
        "button_label": {
          "type": "Text",
          "config": {
            "label": "Button Label",
            "placeholder": "What should the button/link say?"
          }
        },
        "button_variant": {
          "type": "Select",
          "config": {
            "label": "Button Variant",
            "placeholder": "What type of button do you want?",
            "options": [
              "default",
              "secondary",
              "outline",
              "ghost",
              "link",
              "destructive"
            ],
            "default_value": "default"
          }
        }
      }
    }
  ]
}